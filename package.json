{
  "name": "@cloudfour/eslint-plugin",
  "version": "1.0.0",
  "description": "Cloud Four's shareable ESLint configuration",
  "author": "Cloud Four <info@cloudfour.com> (http://cloudfour.com)",
  "homepage": "https://github.com/cloudfour/eslint-config",
  "bugs": "https://github.com/cloudfour/eslint-config/issues",
  "license": "MIT",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudfour/eslint-config.git"
  },
  "contributors": [
    "Erik Jung <erik@cloudfour.com>",
    "Caleb Eby <caleb@cloudfour.com>"
  ],
  "files": [
    ".eslintrc.json",
    "index.js",
    "babel.js"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "dependencies": {
    "@babel/core": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0"
  },
  "peerDependencies": {
    "eslint": "^5.9.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-xo": "^0.25.0",
    "prettier": "^1.15.2"
  },
  "scripts": {
    "check-lint": "eslint . && prettier --list-different '**/*.js'",
    "lint": "eslint . && prettier --write '**/*.js'",
    "prepublishOnly": "npm run check-lint && npm run build",
    "build": "node build.js"
  },
  "eslintConfig": {
    "extends": "plugin:@cloudfour/recommended"
  }
}
